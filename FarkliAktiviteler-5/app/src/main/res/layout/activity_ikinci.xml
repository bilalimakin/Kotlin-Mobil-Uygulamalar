<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IkinciActivity">

    <LinearLayout
        android:id="@+id/UstMenu"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        app:layout_constraintTop_toTopOf="parent"
        android:background="?attr/colorPrimaryDark"
        android:orientation="vertical">
    </LinearLayout>

    <ImageView
        android:id="@+id/solMenuCD"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/UstMenu"
        app:layout_constraintStart_toStartOf="@+id/UstMenu"
        app:layout_constraintTop_toTopOf="@+id/UstMenu"
        app:srcCompat="@android:drawable/ic_menu_sort_by_size" />

    <ImageView
        android:id="@+id/sagMenuCD"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/UstMenu"
        app:layout_constraintEnd_toEndOf="@+id/UstMenu"
        app:layout_constraintTop_toTopOf="@+id/UstMenu"
        app:srcCompat="@android:drawable/ic_menu_myplaces" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E-tüt"
        android:textAlignment="center"
        android:textSize="54sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/AltMenu"
        app:layout_constraintEnd_toEndOf="@+id/AltMenu"
        app:layout_constraintStart_toStartOf="@+id/AltMenu" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/UstMenu"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/UstMenu"
        app:layout_constraintTop_toBottomOf="@+id/UstMenu"
        app:layout_constraintVertical_bias="0.0">


        <!-- İlk sütun, ilk satır -->
        <ImageView
            android:id="@+id/osym10"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            android:onClick="osymKpssGit"
            app:srcCompat="@drawable/osym" />

        <ImageView
            android:id="@+id/osym20"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />

        <ImageView
            android:id="@+id/osym30"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="2"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />

        <ImageView
            android:id="@+id/osym40"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="3"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />



        <!-- İkinci sütun, İkinci satır -->

        <ImageView
            android:id="@+id/osym12"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />

        <ImageView
            android:id="@+id/osym22"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />

        <ImageView
            android:id="@+id/osym32"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="2"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />

        <ImageView
            android:id="@+id/osym42"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="3"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />


        <!-- Üçüncü sütun, Üçüncü satır -->
        <ImageView
            android:id="@+id/osym13"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />

        <ImageView
            android:id="@+id/osym23"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />

        <ImageView
            android:id="@+id/osym33"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="2"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />

        <ImageView
            android:id="@+id/osym43"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_row="3"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_margin="5dp"
            android:contentDescription="KPSSDERSLER"
            app:srcCompat="@drawable/osym" />



    </GridLayout>

    <TextView
        android:id="@+id/baslik_Etüt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E-tüt"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="44sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/UstMenu"
        app:layout_constraintEnd_toEndOf="@+id/UstMenu"
        app:layout_constraintStart_toStartOf="@+id/UstMenu"
        app:layout_constraintTop_toTopOf="@+id/UstMenu" />

    <LinearLayout
        android:id="@+id/AltMenu"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="?attr/colorPrimaryDark"
        app:layout_constraintBottom_toBottomOf="parent">

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>